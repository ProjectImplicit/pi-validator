{
    "rules": {
        "indent": [1,"tab"],
        "quotes": [1,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [1,"always"],
        "no-console": [1]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "globals": {
        "m" : true
    },
    "extends": "eslint:recommended"
}